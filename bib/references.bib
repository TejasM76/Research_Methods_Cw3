@article{YuHuangGuoYang2023ToolWearPrediction,
  author = {Weiwei Yu, Hua Huang, Runlan Guo, and Pengqiang Yang.},
  abstract = {This study proposes a novel approach for tool wear prediction based on an Attention Long Short-term Memory (LSTM) network, particularly suited for scenarios with limited sample data. The model is designed to forecast tool wear in industrial processes, addressing the challenge of small sample sizes. By incorporating attention mechanisms, the LSTM network can effectively capture temporal dependencies and highlight relevant information from the input data. Experimental results demonstrate the effectiveness of the proposed approach in predicting tool wear with high accuracy, even when trained on small datasets. This work contributes to the advancement of predictive maintenance strategies in manufacturing and machining industries.},
  keywords = {tool wear prediction, Attention LSTM network, predictive maintenance, small sample data, manufacturing industry},
  title = {Tool Wear Prediction Based on Attention Long Short-term Memory Network with Small Samples},
  journal = {Sensors and Materials},
  volume = {35},
  number = {7},
  year = {2023},
  doi = {10.18494/SAM4509},
  url = {https://doi.org/10.18494/SAM4509}
}

@article{ZhengEtAl2020Memory,
  author = {LIN ZHENG, NAICHENG GUO, JIN YU, DAZHI JIANG.},
  abstract = {The paper presents a symmetric memory network designed for reorganizing neighbors and topics to enhance rating prediction tasks. This Memory Reorganization model aims to improve the accuracy of rating predictions by efficiently capturing relationships between items and users. By reorganizing the memory of past interactions, the model achieves notable performance improvements in completing rating predictions. The proposed approach offers insights into enhancing recommendation systems through effective memory management and topic organization.},
  keywords = {memory reorganization, recommendation systems, rating prediction, symmetric memory network},
  title = {Memory Reorganization: A Symmetric Memory Network for Reorganizing Neighbors and Topics to Complete Rating Prediction},
  journal = {IEEE Access},
  volume = {8},
  year = {2020},
  doi = {10.1109/ACCESS.2020.2991093},
  url = {https://doi.org/10.1109/ACCESS.2020.2991093}
}

@article{ZhangEtAl2021Weather,
  author = {Lei Zhang, Zhenyue Huang, Wei Liu, Zhongli Guo, Zhe Zhang.},
  abstract = {This study proposes a novel method for weather radar echo prediction, focusing on its application in sustainable agriculture. The method combines Convolutional Neural Networks (CNNs) and Long Short-Term Memory (LSTM) networks to predict radar echoes. Results demonstrate the effectiveness of the approach in accurately predicting weather radar echoes, which is crucial for sustainable agricultural practices. By leveraging CNNs and LSTM networks, this method offers a promising solution for improving weather forecasting systems in the context of agriculture and environmental sustainability.},
  keywords = {weather radar prediction, convolutional neural networks, LSTM networks, sustainable agriculture, weather forecasting},
  title = {Weather Radar Echo Prediction Method Based on Convolution Neural Network and Long Short-Term Memory Networks for Sustainable Agriculture},
  journal = {Journal of Cleaner Production},
  volume = {298},
  pages = {126776},
  year = {2021},
  doi = {10.1016/j.jclepro.2021.126776},
  url = {https://doi.org/10.1016/j.jclepro.2021.126776}
}


@article{LiEtAl2021Detection,
  author = {Fangyu Li , QiLi, Jinan Zhang, Jiabao Kou , JinYe, WenZhan Song, and Homer Alan Mantooth.},
  abstract = {This paper proposes a novel approach for the detection and diagnosis of data integrity attacks in solar farms based on a multilayer long short-term memory (LSTM) network. By utilizing LSTM networks, the system effectively identifies anomalies in solar farm data, indicative of potential integrity attacks. Experimental results demonstrate the effectiveness of the proposed approach in accurately detecting and diagnosing data integrity attacks, thus enhancing the security of solar farm operations.},
  keywords = {data integrity attacks, solar farms, multilayer long short-term memory network, anomaly detection, cybersecurity},
  title = {Detection and Diagnosis of Data Integrity Attacks in Solar Farms Based on Multilayer Long Short-Term Memory Network},
  journal = {IEEE Transactions on Power Electronics},
  volume = {36},
  pages = {2495-2498},
  year = {2021},
  doi = {10.1109/TPEL.2020.3017935},
  url = {https://doi.org/10.1109/TPEL.2020.3017935}
}

@article{MandalSinghThakur2022Epileptic,
  author = {Sunandan Mandal, Bikesh Kumar Singh, Kavita Thakur.},
  abstract = {In this study, Mandal et al. explore the use of deep learning-based Long Short-Term Memory (LSTM) networks and time-frequency analysis for epileptic seizure detection. Through a comparative investigation in the machine learning paradigm, they evaluate the performance of their proposed approach. Results demonstrate the effectiveness of LSTM networks combined with time-frequency analysis in accurately detecting epileptic seizures. The study contributes to advancing the field of epileptic seizure detection by leveraging deep learning techniques and signal processing methods.},
  keywords = {epileptic seizure detection, deep learning, Long Short-Term Memory (LSTM), time-frequency analysis, machine learning},
  title = {Epileptic Seizure Detection Using Deep Learning Based Long Short-Term Memory Networks and Time Frequency Analysis: a Comparative Investigation in Machine Learning Paradigm},
